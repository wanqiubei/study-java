package com.nowcoder.part03;

public class ConversionDemo {

	public static void main(String[] args) {
		// 自动类型转换
		char c = 'A';
		int i = c;
		System.out.println(i);
		
		long l=100;
		double d=l;
		System.out.println(d);
		
		//强制类型转换
		int ii=65;
		char cc=(char) ii;
		System.out.println(ii);
	}

}
